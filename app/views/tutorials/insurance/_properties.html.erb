<%- properties = Vendor.for_user(resource_owner).properties %>
<%- properties.each do |property| %>
  <div class="collect-fields">
    <%= hidden_field_tag :tutorial_name, @tutorial_name, class: 'saved-field' %>
    <div class="input-pair tutorial-fields property-casualty-fields">
      <%= text_field_tag "property[name]", property.name, class: "saved-field form-control medium", placeholder: 'Insurance Provider Name', :maxlength => get_max_length(:default) %>
      <a data-id=<%= property.id %> class="medium-button outline-button inline-button remove-btn">Remove</a>
    </div>
  </div>
<% end %>
