<%= hidden_field_tag :shared_user_id, @shared_user.try(:id) %>
<%= render :partial => "layouts/add_document_button", :locals => {documents_path: new_document_path(:shared_user_id => @shared_user.present? ? @shared_user.id : ""), documents_title: "Documents", :title => true } %>
<%= render :partial => "layouts/standard_document_table", :locals => {:documents => @documents} %>

<div class="mt-30">
  <%= render :partial => "layouts/shared_category", :locals => { :category => Rails.application.config.x.DocumentsCategory.downcase } %>
</div>
