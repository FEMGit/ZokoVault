<%- lives = Vendor.for_user(resource_owner).lives %>
<%- lives.each do |life| %>
  <div class="collect-fields">
    <%= hidden_field_tag :tutorial_name, @tutorial_name, class: 'saved-field' %>
    <div class="input-pair tutorial-fields life-disability-fields">
      <%= text_field_tag "life[name]", life.name, class: "saved-field form-control medium", placeholder: 'Insurance Provider Name', :maxlength => get_max_length(:default) %>
      <a data-id=<%= life.id %> class="medium-button outline-button inline-button remove-btn">Remove</a>
    </div>
  </div>
<% end %>
