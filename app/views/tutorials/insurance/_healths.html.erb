<%- healths = Vendor.for_user(resource_owner).healths %>
<%- healths.each do |health| %>
  <div class="collect-fields">
    <%= hidden_field_tag :tutorial_name, @tutorial_name, class: 'saved-field' %>
    <div class="input-pair tutorial-fields health-fields">
      <%= text_field_tag "health[name]", health.name, class: "saved-field form-control medium", placeholder: 'Insurance Provider Name', :maxlength => get_max_length(:default) %>
      <a data-id=<%= health.id %> class="medium-button outline-button inline-button remove-btn">Remove</a>
    </div>
  </div>
<% end %>
