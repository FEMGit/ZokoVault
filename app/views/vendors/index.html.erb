<p id="notice"><%= notice %></p>

<h1>Listing Vendors</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Group</th>
      <th>Name</th>
      <th>Webaddress</th>
      <th>Phone</th>
      <th>Contact</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vendors.each do |vendor| %>
      <tr>
        <td><%= vendor.category %></td>
        <td><%= vendor.group %></td>
        <td><%= vendor.name %></td>
        <td><%= vendor.webaddress %></td>
        <td><%= vendor.phone %></td>
        <td><%= vendor.contact %></td>
        <td><%= vendor.user %></td>
        <td><%= link_to 'Show', vendor %></td>
        <td><%= link_to 'Edit', edit_vendor_path(vendor) %></td>
        <td><%= link_to 'Destroy', vendor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vendor', new_vendor_path %>
